prototype(Era.Site:Document.Page) < prototype(Era.Site:Document.AbstractPage) {
    body.content = Neos.Fusion:Component {
        main = Neos.Neos:ContentCollection {
            nodePath = 'main'
        }

        renderer = afx`
            <main class="main">
                {props.main}
            </main>
        `
    }
}
